{
  "name": "stylesheet-observer",
  "version": "1.0.1",
  "description": "Observer for changes made to StyleSheetLists",
  "main": "index.js",
  "scripts": {
    "test": "karma start karma/local.karma.conf.js --single-run",
    "test:browserstack": "karma start karma/browserstack.karma.conf.js --single-run",
    "test:watch": "karma start karma/karma.conf.js",
    "build": "npm run build:npm && npm run build:bower",
    "build:npm": "babel src --out-dir lib",
    "build:bower": "webpack --config webpack/bower.webpack.conf.js"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "bdd-lazy-var": "^1.2.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.6.0",
    "eslint-config-foobarhq": "git@bitbucket.org:foobarhq/eslint-config-foobarhq.git",
    "istanbul-instrumenter-loader": "^1.0.0",
    "karma": "^1.3.0",
    "karma-browserstack-launcher": "^1.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "webpack": "^1.13.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foobarhq/stylesheer-observer.git"
  },
  "keywords": [
    "DOM",
    "StyleSheetList",
    "StyleSheet",
    "Observer"
  ],
  "author": "Guylian Cox <gc@pictawall.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foobarhq/stylesheer-observer/issues"
  },
  "homepage": "https://github.com/foobarhq/stylesheer-observer#readme"
}
